//--------------------------------------------------------------------------------------
// Include Files
//--------------------------------------------------------------------------------------
#include <stdio.h>

//-----------------------------------------------------------------
// DEFINES
//-----------------------------------------------------------------
#define BITSPERWORD 32  
#define SHIFT 5  
#define MASK 0x1F  
#define N 10000000 	
int a[1 + N/BITSPERWORD];  //申请内存的大小 

//-----------------------------------------------------------------

//----------------------------------------------------------------------------
void set(int i) 
{        
      a[i>>SHIFT]=a[n>>SHIFT]|(1<<(i&MASK));  //set 设置所在的bit位为1
}  

void clr(int i) 
{        
      a[i>>SHIFT]=a[n>>SHIFT]&(~(1<<(i&MASK)));  //clr 初始化所有的bit位为0
}  

int  test(int i)
{     
      return a[i>>SHIFT]&(1<<(i&MASK)); //test 测试所在的bit为是否为1
} 

int main()  
{   
    int i;  
    for (i = 0; i < N; i++)  
         clr(i);    
    while (scanf("%d", &i) != EOF)  
         set(i);  
    for (i = 0; i < N; i++)  
         if (test(i))  
             printf("%d\n", i);  
             return 0;  
}

